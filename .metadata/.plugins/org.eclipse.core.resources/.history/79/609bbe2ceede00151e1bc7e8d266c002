import org.springframework.web.servlet.ModelAndView;

@Controller
@RequestMapping("/test")
public class HelloWorldController {

	
	@RequestMapping(value="helloWorld",method=RequestMethod.GET)
	public ModelAndView  helloWorld(Model model){
